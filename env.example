# SparkBoard Frontend Environment Variables
# Copy this file to .env.local and fill in your values from CDK outputs

# Cognito Configuration
VITE_AWS_REGION=YOUR AWS REGION HERE
VITE_USER_POOL_ID=YOUR Cognito USER POOL ID HERE
VITE_USER_POOL_CLIENT_ID=YOUR Cognito APP CLIENT ID HERE
VITE_COGNITO_DOMAIN=YOUR Cognito DOMAIN HERE

# API Configuration
VITE_API_BASE_URL=YOUR API BASE URL HERE

# OAuth URLs (for Hosted UI)
VITE_OAUTH_REDIRECT_URI=http://localhost:5173/callback
VITE_OAUTH_LOGOUT_URI=http://localhost:5173
